{
  "name": "typescript-agi",
  "version": "0.0.7",
  "description": "NodeJS TypeScript Library for Asterisk AGI Interfaces",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "docs": "./node_modules/.bin/typedoc && touch docs/.nojekyll",
    "fix-style": "./node_modules/.bin/tslint -c tslint.json --project . --fix",
    "prepare": "npm run build",
    "style": "./node_modules/.bin/tslint -c tslint.json --project .",
    "test": "npm run style && npm run build"
  },
  "author": "Brandon Lehmann <brandonlehmann@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^15.0.2",
    "tslint": "^6.1.3",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandonlehmann/typescript-agi.git"
  },
  "homepage": "https://github.com/brandonlehmann/typescript-agi#readme",
  "bugs": {
    "url": "https://github.com/brandonlehmann/typescript-agi/issues"
  }
}
